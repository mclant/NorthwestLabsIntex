@model IEnumerable<NorthwestLabs.Models.AssayTypes>

@{
    ViewBag.Title = "Pharmacology Catalog";
}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")">Home</a></li>
        <li class="breadcrumb-item"><a href="@Url.Action("Services","Services")">Services</a></li>
        <li class="breadcrumb-item active" aria-current="page">Pharmacology Catalog</li>
    </ol>
</nav>
<h2>Pharmacology Catalog</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AssayName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssayProtocol)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AssayName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssayProtocol)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.AssayTypeID })
        </td>
    </tr>
}

</table>
